<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="SimpleApp.Views.DetailCarView">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="{Binding Model.Model}"/>
            <Label Text="{Binding Model.Year}"/>
            <Label Text="{Binding Model.Mileage}"/>
            <Label Text="{Binding Model.Description}"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>